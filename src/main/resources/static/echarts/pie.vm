{
    title: {
        text: '$title',
        left: 'center'
    },
    tooltip: {
        trigger: 'item',
        formatter: '{a} <br/>{b}: {c} ({d}%)'
    },
    legend: {
        orient: 'vertical',
        right: 30,
        data: $legendData
    },
    series: [{
        type: 'pie',
        radius: '65%',
        center: ['50%', '50%'],
        label: {
            formatter: '{b} ({c}, {d}%)'
        },
        data: $seriesData
    }]
}